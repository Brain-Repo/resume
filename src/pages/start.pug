header.page-layout-size.layout-shadow
    img.photo(src=require('../img/photo.jpg'))
    .present
        h1
            span.surname Пилипенко  
            span.name Анатолий Юрьевич
        hr
        .weblinks
            a.git-link.weblinks-text-style(href='https://github.com/Brain-Repo/resume') Github
            a.facebook-link.weblinks-text-style Gitlab
            a.site-link.weblinks-text-style Facebook
            a.youtube-link.weblinks-text-style Telegram
        hr
section.page.page-layout-size.layout-shadow.page-layout-margin-top
    section.sidebar
            article
                h1.sidebar-item-head Общие навыки
                .sidebar-content.sidebar-content-skills
                h1.sidebar-item-head Специальные навыки
                .sidebar-content.sidebar-content-specscills
                h1.sidebar-item-head Контакты
                .sidebar-content.sidebar-content-contacts
                    .sidebar-content-item
                        img.contact(src=require('../ico/Phone.svg'))
                        span +(555)333-22-11 +(555)444-33-22 +(555)444-33-22
                    .sidebar-content-item
                        img.contact(src=require('../ico/mail.svg'))
                        span p330543@gmail.com
                    .sidebar-content-item
                        img.contact(src=require('../ico/location.svg'))
                        span ул.Улица, д. дом кв. квартира
    main.content
        article.education
            h1.content-item-head Образование

            each val,key in edutext
                .article-item
                    aside.article-aside=key
                    p.article-content-zeromargin.article-content=val

        article.work
            h1.content-item-head Трудовая деятельность
            h1.content-item-subhead.
                            ОАО "Укртелеком" Центр технической эксплуатации первичной сети №3
            each val,key in worktexttelecom
                .article-item
                    aside.article-aside=key
                    p.article-content-zeromargin.article-content=val


            h1.content-item-subhead ООО "Дельта-телеком"
            each val,key in worktextdelta
                .article-item
                    aside.article-aside=key
                    p.article-content-zeromargin.article-content=val



            h1.content-item-subhead ООО "Ист.Нет"
            each val,key in worktexteast
                .article-item
                    aside.article-aside=key
                    p.article-content-zeromargin.article-content=val
        article.courses
            h1.content-item-head Курсы
            h1.content-item-subhead ОАО "Укртелеком", филиал "Центр последипломного образования"
            each val,key in coursestext
                .article-item
                    aside.article-aside=key
                    p.article-content-zeromargin.article-content=val



footer.page-layout-size.page-layout-margin-top.layout-shadow

